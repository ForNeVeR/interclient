# the id of the signer
issuer.name=InterBaseSoftwareCorp

# the cert to use for the signing
issuer.cert=1

# the id of the subject
subject.name=InterBaseSoftwareCorp

# the components of the X500 name for the subject
subject.real.name=InterBaseSoftwareCorp
subject.org.unit=InterBase Software Corporation
subject.org=Borland International
subject.country=US

# Various parameters: start and end date for validity and expiration
# of the certificate. Serial number. FIle to which to output the
# certificate (optional).
start.date=17 Jan 1997
end.date=16 Jan 1998
serial.number=1001
out.file=InterBaseSoftwareCorp.x509
